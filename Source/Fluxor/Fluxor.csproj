<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;netcoreapp3.1</TargetFrameworks>
		<Version>3.7.0</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Peter Morris</Authors>
		<Company />
		<Description>A zero boilerplate Redux/Flux framework for DotNet</Description>
		<Copyright>Peter Morris</Copyright>
		<PackageProjectUrl>https://github.com/mrpmorris/Fluxor</PackageProjectUrl>
		<PackageIcon>fluxor-small-logo.png</PackageIcon>
		<PackageIconUrl />
		<RepositoryUrl>https://github.com/mrpmorris/Fluxor</RepositoryUrl>
		<PackageTags>Redux Flux DotNet CSharp</PackageTags>
		<PackageLicenseFile></PackageLicenseFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryType>git</RepositoryType>
		<AssemblyVersion>3.7.0.0</AssemblyVersion>
		<FileVersion>3.7.0.0</FileVersion>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>MrPMorris.snk</AssemblyOriginatorKeyFile>
		<DelaySign>false</DelaySign>
	</PropertyGroup>

	<PropertyGroup>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\Images\fluxor-small-logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.6" />
	</ItemGroup>

</Project>
