<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>3.1.0-Pre-2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Peter Morris</Authors>
    <Company />
    <Description>A zero boilerplate Redux/Flux framework for DotNet</Description>
    <Copyright>Peter Morris</Copyright>
    <PackageProjectUrl>https://github.com/mrpmorris/Fluxor</PackageProjectUrl>
    <PackageIcon>fluxor-small-logo.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/mrpmorris/Fluxor</RepositoryUrl>
    <PackageTags>Redux Flux DotNet CSharp</PackageTags>
    <PackageLicenseFile></PackageLicenseFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <AssemblyVersion>3.1.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\Images\fluxor-small-logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
